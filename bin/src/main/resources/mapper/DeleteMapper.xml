<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.visa.kigen.mapper.DeleteMapper">
    <delete id="delete" parameterType="com.visa.kigen.model.DeleteModel">
    DELETE FROM b_visa WHERE id = #{id};
</delete>

    <select id="selectuser" resultType="com.visa.kigen.model.DeleteModel">
        SELECT * FROM b_visa WHERE id=#{id}
    </select>
</mapper>